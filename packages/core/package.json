{
  "name": "@designer/core",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "main": "dist/designer.core.umd.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "rimraf -rf dist && npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:esnext",
    "build:cjs": "tsc --project ./tsconfig.build.json --module CommonJS --target ES5 --outDir ./dist/cjs",
    "build:esm": "tsc --project ./tsconfig.build.json --module ES6 --target ES5 --outDir ./dist/esm",
    "build:esnext": "tsc --project ./tsconfig.build.json --module ESNext --target ESNext --outDir ./dist/esnext",
    "build:umd": "rollup --config",
    "dev": "microbundle watch -i src/index.ts --external none --compress false -f umd --generateTypes false --sourcemap false --output dist/designer.core.umd.js --name Designer.Core"
  },
  "dependencies": {
    "@designable/shared": "1.0.0-beta.45",
    "@juggle/resize-observer": "^3.3.1"
  },
  "devDependencies": {
    "@formily/json-schema": "^2.0.2",
    "@formily/path": "^2.0.2",
    "@formily/reactive": "^2.0.2"
  },
  "peerDependencies": {
    "@formily/json-schema": "^2.0.2",
    "@formily/path": "^2.0.2",
    "@formily/reactive": "^2.0.2"
  },
  "engines": {
    "npm": ">=3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "esnext": "dist/esnext/index.js"
}
